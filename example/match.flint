@external(c, "flint_stdlib", "print")
pub fn print(string: String) Nil

fn test(i: Int) String {
    match i {
        | 1 -> "Hello"
        | x if x == 2 -> "Bye"
        | _ -> match i == 10 {
            | True -> "true"
            | False -> "false"
        }
    }
}

pub fn main() Nil {
    print(test(3))
    print("\n")
}